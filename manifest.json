{
  "manifest_version": 3,
  "name": "Mooodz",
  "version": "1.0.0",
  "description": "Motivational Music",
  "icons": {
    "16": "moodz.png",
    "48": "moodz.png",
    "128": "moodz.png"
  },
  
    "action" : {
    "default_title": "Click to view a popup",
    "default_popup": "popup.html"
  },
    "background" : {
        "service_worker" : "background.js"
  
},
    "permissions": ["storage", "tabs"], 
    "host_permissions": ["https://www.youtube.com/*"] // allows access to external websites 
}
